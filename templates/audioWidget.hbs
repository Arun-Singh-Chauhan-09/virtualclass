<div id="audioWidget">
  <div id="mainAudioPanel">
    <div id="speakerPressOnce" class="audioTool deactive" data-audio-playing="false">
      <a id="speakerPressonceAnch" class="tooltip" data-title="audio_tooltip" name="speakerPressonceAnch">
        <span id="speakerPressonceLabel" class="silenceDetect" data-silence-detect="stop"> <i> </i> </span>
      </a>
    </div>
    <div id="videoPacketInfo">
      <span id="videoSpeed">
      <span id="videSpeedNumber" class="suggestion tooltip" data-suggestion="suggestion"
            data-title="proposedspeed">  </span>
      <span id="videLatency" class="latency  tooltip" data-latency="latency"
            data-title="audiolatency">  </span>
      <span id="videoFrameRate" class="quality  tooltip" data-quality="quality"
            data-title="videoquality"> </span>
      </span>
    </div>
  </div>
</div>