{{!-- note navigation --}}
<div class="linknotes links" id="link{{id}}" data-screen="{{id}}" data-rid="{{rid}}" data-selected="0"
     data-status="{{status}}" draggable="true">
  <div class="mainpreview" id="mainp{{id}}" data-screen="{{id}}" data-rid="{{rid}}">
    <img class="thumbnail" id="thumbnail{{rid}}"
         style="border: 1px solid gray; width: 70px; height: 40px; padding: 5px;">
    <span class="thumbList tooltip2" data-title="{{title}}">{{thumbCount}}</span>
  </div>
  <div id="controlCont{{id}}" class="controlCont">
    <div class="controls status" data-status="{{status}}">
      <a class="statusanch">status{{status}}</a>
    </div>
    <div class="controls delete">
      <a class="deleteanch">delete</a>
    </div>
  </div>
</div>
